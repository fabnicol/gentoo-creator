#!/bin/bash
if ! test -d "${VMPATH}"; then
	echo "VMPATH=${VMPATH} is not a directory. Export this variable first"
	exit -1
fi
LD_LIBRARY_PATH="${VMPATH}/virtualbox/VirtualBox-${VBOX_VERSION}/out/linux.amd64/release/bin/:${VMPATH}/virtualbox/lib:${VMPATH}/virtualbox/lib32:${VMPATH}/virtualbox/lib64" /bin/bash ${VMPATH}/virtualbox/VirtualBox-6.1.14/out/linux.amd64/release/bin/vbox-img $*
cd - >/dev/null
